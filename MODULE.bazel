# Test regular bazel_dep (should appear in bazel.txt)
bazel_dep(name = "test-missing-package", version = "1.0")
bazel_dep(name = "another-fake-pkg", version = "2.0")

# Test workspace path override (should create BAZEL_MODULE_OVERRIDE_CONFUSION)
local_path_override(
    module_name = "local-missing-module", 
    path = "./non-existent-path"
)

