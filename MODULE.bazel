    name = "test_repo",
    remote = "https://github.com/fake-org-123/missing-repo-456.git"
)

# Test Maven artifact (should appear in bazel.txt)
maven_install(
    artifacts = [
        "com.fake:missing-artifact:1.0",
        "org.nonexistent:fake-lib:2.0"
    ]
)
